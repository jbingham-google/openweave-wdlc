WDLC_VERSION_REGEXP := \([0-9]\{1,\}\)\.\([0-9]\{1,\}\)\.*\([0-9]\{1,\}\)*\(a\|b\|d\|rc\)*\([0-9]\{1,\}\)*-*\(.*\)*$$
WDLC_VERSION_EXTRACT = $(shell echo "$(WDLC_VERSION)" | $(SED) -n -e "s/$(WDLC_VERSION_REGEXP)/\$1/gp")
WDLC_VERSION_MAJOR = $(call WDLC_VERSION_EXTRACT,1)
